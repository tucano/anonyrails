<div id='request-list'>

<h1>request variables</h1>

<table id="request-table" summary="Request Environment Info">
  <thead>
    <tr>
      <th scope="col">Key</th>
      <th scope="col">Value</th>
    </tr>
  </thead>
  <tbody>
  <% for item in @request_vars %>
    <tr valign="top" class="<%= cycle('list-line-odd','list-line-even') %>">
      <td><%= item[0] %></td>
      <td><%= item[1] %></td>
    </tr>
  <% end %>
  </tbody>
</table>

</div>
